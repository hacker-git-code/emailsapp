        "ecommerce": {
            "light": "<!DOCTYPE html>\n<html>\n<head>\n  <title>E-commerce Store</title>\n  <script src='https://cdn.jsdelivr.net/npm/react@18.2.0/umd/react.development.js'></script>\n  <script src='https://cdn.jsdelivr.net/npm/react-dom@18.2.0/umd/react-dom.development.js'></script>\n  <script src='https://cdn.tailwindcss.com'></script>\n  <style>\n    body { font-family: 'Inter', sans-serif; }\n  </style>\n</head>\n<body class='bg-gray-100'>\n  <div id='root'></div>\n  <script type='text/babel'>\n    const App = () => {\n      const products = [{id: 1, name: 'Product 1', price: 29.99, img: 'https://via.placeholder.com/150'}, {id: 2, name: 'Product 2', price: 49.99, img: 'https://via.placeholder.com/150'}];\n      return (\n        <div className='container mx-auto p-6'>\n          <h1 className='text-4xl font-bold mb-6'>Our Store</h1>\n          <div className='grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 gap-6'>\n            {products.map(product => (\n              <div key={product.id} className='bg-white p-4 rounded-xl shadow-lg hover:shadow-xl transition-transform transform hover:scale-105'>\n                <img src={product.img} alt={product.name} className='w-full h-48 object-cover rounded-lg' />\n                <h2 className='text-xl font-semibold mt-3'>{product.name}</h2>\n                <p className='text-gray-600'>${product.price}</p>\n                <button className='mt-3 bg-blue-600 text-white px-4 py-2 rounded-lg hover:bg-blue-700'>Add to Cart</button>\n              </div>\n            ))}\n          </div>\n        </div>\n      );\n    };\n    ReactDOM.render(<App />, document.getElementById('root'));\n  </script>\n</body>\n</html>",
